# ======================================
#  .gitignore for Full Stack (React + Node)
# ======================================

# React dependencies
node_modules/

# Backend dependencies (if backend folder exists)
backend/node_modules/

# Build outputs
build/
dist/
backend/build/

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
logs
*.log

# Test coverage
coverage/

# IDE and system files
.vscode/
.idea/
.DS_Store
Thumbs.db

# Cache / Temporary
*.tmp
*.swp

# Yarn Plug’n’Play
.pnp
.pnp.js

